pipeline {
   agent any
     stages {
        stage('clone') {
           steps {
           git branch:'main', url: 'git@github.com:245123737105-art/calculator1.git';
          }
        }
        stage('compile') {
           steps {
           sh 'javac calculator.java'
          }
        }
        stage('build') {
           steps {
           sh 'java Calculator 25 5'
           }
        }
       stage('test') {
           steps {
           sh 'java Calculator 30 -5'
           }
        }
       stage('Deploy') {
           steps {
           echo 'Deployment completed'
           }
        }
}
}
